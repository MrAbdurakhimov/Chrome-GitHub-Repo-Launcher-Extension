{
  "manifest_version": 3,

  "name": "GitHub Repository Explorer",
  "version": "1.0.0",

  "action": {
    "default_icon": "github.png"
  },

  "background": {
    "service_worker": "eventPage.js"
  },

  "icons": {
    "256": "github.png"
  },

  "commands": {
    "launchVSCode": {
      "suggested_key": {
        "default": "Ctrl+Period",
        "mac": "Command+Period"
      },
      "description": "GitHub Repository"
    }
  },

  "permissions": ["tabs", "history", "storage"],

  "omnibox": { "keyword": "repo" }
}
